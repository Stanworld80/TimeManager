<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#e1e7f0</item>
        <item name="colorPrimaryDark">#9299a6</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@android:color/holo_orange_light</item>
        <item name="android:textColor">@android:color/holo_orange_light</item>
        <item name="android:textColorPrimary">@android:color/black</item>
    </style>

    <style name="TimeManagerBarTitle" parent="TextAppearance.AppCompat.Title">
    <item name="android:textSize">@dimen/abc_text_size_title_material_toolbar</item>
    <item name="android:textColor">#0000ff</item>
    </style>
</resources>
